{
    "CommonRes": {
        "cmakeTemplate": "cmake_minimum_required(VERSION 3.10)\nset(CMAKE_CXX_STANDARD 17)\nset(CMAKE_CXX_STANDARD_REQUIRED ON)\nproject({repoName} VERSION 1.0)\nadd_executable(\"${{PROJECT_NAME}}\" \"main.cpp\")",
        "tmpDir": "./",
        "patchGeneratorPrefix": "const char \\*patchGenerator = R\"V0G0N\\(",
        "patchGeneratorPostfix": "\\)V0G0N\";"
    },
    "FirstBadVersionTestRes": {
        "cfg": {
            "appCmd" : "{appCmd}",
            "appPath": "{appPath}",
            "gitPath" : "{gitPath}",
            "buildPath" : "{buildPath}",
            "verboseOutput": false,
            "runConfig" : {
                "commitList" : {
                    "getCommitListCmd" : "git log {start}..{end} --boundary --pretty=\"%h\""
                },
                "mode" : "checkOutput",
                "traversal" : "firstFailedVersion",
                "stopPattern" : "(.)*failed(.)*"
            }
        }
    }
}